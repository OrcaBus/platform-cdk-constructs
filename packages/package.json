  "description": "CDK Package for OrcaBus infrastructure.",
  "main": "index.ts",
  "types": "index.d.ts",
  "scripts": {
    "build": "jsii --tsconfig tsconfig.json --validate-tsconfig minimal",
    "build:watch": "jsii --watch",
