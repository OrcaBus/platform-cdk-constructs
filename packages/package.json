    "test": "pnpm jest"
  },
  "keywords": [],
  "repository": {
    "type": "git",
    "url": "https://github.com/OrcaBus/platform-cdk-construct.git",
    "directory": "packages"
  },
  "license": "MIT",
  "author": {
    "name": "University of Melbourne Centre for Cancer Research",
    "organization": true
  },
  "devDependencies": {
    "aws-cdk": "^2.1006.0",
    "aws-cdk-lib": "2.195.0",
    "@aws-cdk/aws-lambda-python-alpha": "2.195.0-alpha.0",
    "@types/node": "^22.13.10",
    "constructs": "10.4.2",
    "jsii": "^5.8.3",
    "jsii-pacmak": "^1.110.0",
    "publib": "^0.2.965",
    "typedoc": "^0.28.2",
    "typedoc-plugin-markdown": "^4.6.2",
    "@types/jest": "^29.5.14",
    "jest": "^29.7.0",
    "ts-jest": "^29.3.0"
  },
  "peerDependencies": {
    "aws-cdk-lib": "^2.195.0",
    "constructs": "^10.4.2",
    "@aws-cdk/aws-lambda-python-alpha": "^2.195.0-alpha.0"
  },
  "stability": "experimental",
  "jsii": {
    "outdir": "dist",
    "versionFormat": "short",
    "targets": {},
    "excludeTypescript": [
      "dev/**/*.ts"
